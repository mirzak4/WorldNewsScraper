{
  "Serilog": {
    "Using": [ "Serilog.Sinks.MSSqlServer" ],
    "MinimumLevel": "Information",
    "WriteTo": [
      {
        "Name": "MSSqlServer",
        "Args": {
          "connectionString": "paste_your_connection_string_here",
          "schemaName":  "scraping",
          "tableName": "Logs",
          "autoCreateSqlTable": true
        }
      }
    ]
  },
  "Database": {
    "ConnectionString": "paste_your_connection_string_here"
  },
  "AllowedHosts": "*"
}

